{
  "name": "vue-test-helpers",
  "version": "1.0.4",
  "description": "Helpers for vue-test-utils",
  "repository": "phanan/vue-test-helpers",
  "main": "dist/vue-test-helpers.min.js",
  "scripts": {
    "lint": "eslint src",
    "test": "yarn lint && mocha-webpack --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js",
    "build": "NODE_ENV=production && BABEL_ENV=production && webpack"
  },
  "keywords": [
    "vue",
    "test",
    "testing",
    "unit test",
    "helpers"
  ],
  "author": {
    "name": "Phan An",
    "email": "me@phanan.net",
    "url": "https://phanan.net"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "minify"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "peerDependencies": {
    "@vue/test-utils": "^1.0.0-beta.10"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-minify": "^0.2.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.8",
    "deep-equal": "^1.0.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^22.1.0",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.1",
    "sinon": "^4.1.6",
    "sinon-chai": "^2.14.0",
    "vue": "^2.5.13",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "np": "^2.18.3"
  }
}
